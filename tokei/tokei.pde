}


void tokei(float width, float height, float size){
  translate(width, height);
  scale(size);
  
  for (int i=0; i<=imax; i++){
    thetatheta = theta + omega*dt;
    omegaomega = omega - g/l*sin(theta)*dt;
    theta = thetatheta;
    omega = omegaomega;
  }
   
  translate(width/2, height/2);
  
  //huriko   
  pushMatrix();
  scale(1, -1);
  background(#d3e4f5);
  stroke(0);
  strokeWeight(3);
  phi = 3*PI/2+theta;
  line(l*cos(phi)-3, l*sin(phi), -3, 0);
  line(l*cos(phi)+3, l*sin(phi), 3, 0);
  fill(#5c4641);
  ellipse(l*cos(phi), l*sin(phi)-10, 2*r, 2*r);
  popMatrix();
  
  //tokei
  ////jyunbi
  float s = second();
  float m = minute() + (s/60.0);
  float h = hour()%12 + (m/60.0);
